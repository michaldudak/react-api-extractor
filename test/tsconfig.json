{
	"compilerOptions": {
		"rootDir": "./",
		"target": "ES2022",
		"moduleResolution": "node16",
		"module": "node16",
		"jsx": "react-jsx",

		"strict": true,
		"noEmit": true,

		"esModuleInterop": true,
		"isolatedModules": true,

		"paths": {
			"react-api-extractor": ["../src/index.ts"]
		}
	},
	"include": ["."],
	"references": [{ "path": "../tsconfig.json" }]
}
